<body>
    <a href="javascript:void(0);" onclick="openNewTab()">PROXIMA ETAPA</a>
    
    <script>
        function openNewTab() {
            var novaJanela = window.open('', '_blank');
            novaJanela.document.write('<html><head><title>ENIGMA - Próxima Etapa</title></head><body><p>Bem-vindo à próxima etapa do enigma!</p></body></html>');
            novaJanela.document.close();
        }
    </script>
</body>
</html>
